<odoo>
    <data noupdate="1">
        <record id="ir_cron_ott_archive_expired" model="ir.cron">
            <field name="name">OTT: Archive Expired Events</field>
            
            <field name="model_id" search="[('model', '=', 'ott.rate.template')]"/>
            
            <field name="state">code</field>
            <field name="code">model._cron_archive_expired_rates()</field>
            
            <field name="user_id" ref="base.user_root"/>
            
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            
            <field name="active" eval="True"/>
        </record>
    </data>
</odoo>